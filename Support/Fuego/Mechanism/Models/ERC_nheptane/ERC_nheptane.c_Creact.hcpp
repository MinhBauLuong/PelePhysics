/*Returns mean enthalpy of mixture in mass units */
__device__ void ckhbms_d(double * T, double * y_wk, double * hbms)
{
    double result = 0;
    double imw[29];/* inv molecular weight array */
    double tT = *T; /*temporary temperature */
    double tc[] = { 0, tT, tT*tT, tT*tT*tT, tT*tT*tT*tT }; /*temperature cache */
    double hml[29], tmp[29]; /* temporary storage */
    double RT = 8.31451e+07*tT; /*R*T */
    speciesEnthalpy_d(hml, tc);
    imolecularWeight_d(imw);
    int id;
    for (id = 0; id < 29; ++id) {
        tmp[id] = y_wk[id]*hml[id]*imw[id];
    }
    for (id = 0; id < 29; ++id) {
        result += tmp[id];
    }

    *hbms = result * RT;
}


/*get mean internal energy in mass units */
__device__ void ckubms_d(double * T, double * y_wk, double * ubms)
{
    double result = 0;
    double imw[29];/* inv molecular weight array */
    double tT = *T; /*temporary temperature */
    double tc[] = { 0, tT, tT*tT, tT*tT*tT, tT*tT*tT*tT }; /*temperature cache */
    double ums[29]; /* temporary energy array */
    double RT = 8.31451e+07*tT; /*R*T */
    speciesInternalEnergy_d(ums, tc);
    imolecularWeight_d(imw);
    /*perform dot product + scaling by wt */
    result += y_wk[0]*ums[0]*imw[0]; /*NC7H16 */
    result += y_wk[1]*ums[1]*imw[1]; /*O2 */
    result += y_wk[2]*ums[2]*imw[2]; /*CO2 */
    result += y_wk[3]*ums[3]*imw[3]; /*H2O */
    result += y_wk[4]*ums[4]*imw[4]; /*CO */
    result += y_wk[5]*ums[5]*imw[5]; /*H2 */
    result += y_wk[6]*ums[6]*imw[6]; /*OH */
    result += y_wk[7]*ums[7]*imw[7]; /*H2O2 */
    result += y_wk[8]*ums[8]*imw[8]; /*HO2 */
    result += y_wk[9]*ums[9]*imw[9]; /*H */
    result += y_wk[10]*ums[10]*imw[10]; /*O */
    result += y_wk[11]*ums[11]*imw[11]; /*CH3O */
    result += y_wk[12]*ums[12]*imw[12]; /*CH2O */
    result += y_wk[13]*ums[13]*imw[13]; /*HCO */
    result += y_wk[14]*ums[14]*imw[14]; /*CH2 */
    result += y_wk[15]*ums[15]*imw[15]; /*CH3 */
    result += y_wk[16]*ums[16]*imw[16]; /*CH4 */
    result += y_wk[17]*ums[17]*imw[17]; /*C2H3 */
    result += y_wk[18]*ums[18]*imw[18]; /*C2H4 */
    result += y_wk[19]*ums[19]*imw[19]; /*C2H5 */
