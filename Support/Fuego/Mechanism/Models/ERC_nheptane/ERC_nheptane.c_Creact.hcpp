    result += y_wk[24]*ums[24]*imw[24]; /*C7H15-2 */
    result += y_wk[25]*ums[25]*imw[25]; /*C7H15O2 */
    result += y_wk[26]*ums[26]*imw[26]; /*C7KET12 */
    result += y_wk[27]*ums[27]*imw[27]; /*C5H11CO */
    result += y_wk[28]*ums[28]*imw[28]; /*N2 */

    *ubms = result * RT;
}


/*compute the production rate for each species */
__device__ void ckwc_d(double * T, double * C, double * wdot)
{
    int id; /*loop counter */

    /*convert to SI */
    for (id = 0; id < 29; ++id) {
        C[id] *= 1.0e6;
    }

    /*convert to chemkin units */
    productionRate_d(wdot, C, *T);

    /*convert to chemkin units */
    for (id = 0; id < 29; ++id) {
        C[id] *= 1.0e-6;
        wdot[id] *= 1.0e-6;
    }
}

