/*Compute P = rhoRT/W(x) */
__device__ void ckpx_d(double * rho, double * T, double * x, double * P)
{
    double XW = 0;/* To hold mean molecular wt */
    XW += x[0]*100.205570; /*NC7H16 */
    XW += x[1]*31.998800; /*O2 */
    XW += x[2]*44.009950; /*CO2 */
    XW += x[3]*18.015340; /*H2O */
    XW += x[4]*28.010550; /*CO */
    XW += x[5]*2.015940; /*H2 */
    XW += x[6]*17.007370; /*OH */
    XW += x[7]*34.014740; /*H2O2 */
    XW += x[8]*33.006770; /*HO2 */
    XW += x[9]*1.007970; /*H */
    XW += x[10]*15.999400; /*O */
    XW += x[11]*31.034460; /*CH3O */
    XW += x[12]*30.026490; /*CH2O */
    XW += x[13]*29.018520; /*HCO */
    XW += x[14]*14.027090; /*CH2 */
    XW += x[15]*15.035060; /*CH3 */
    XW += x[16]*16.043030; /*CH4 */
    XW += x[17]*27.046210; /*C2H3 */
    XW += x[18]*28.054180; /*C2H4 */
    XW += x[19]*29.062150; /*C2H5 */
    XW += x[20]*40.065330; /*C3H4 */
    XW += x[21]*41.073300; /*C3H5 */
    XW += x[22]*42.081270; /*C3H6 */
    XW += x[23]*43.089240; /*C3H7 */
    XW += x[24]*99.197600; /*C7H15-2 */
    XW += x[25]*131.196400; /*C7H15O2 */
    XW += x[26]*146.187830; /*C7KET12 */
    XW += x[27]*99.153970; /*C5H11CO */
    XW += x[28]*28.013400; /*N2 */
    *P = *rho * 8.31451e+07 * (*T) / XW; /*P = rho*R*T/W */

    return;
}


/*Compute P = rhoRT/W(y) */
__device__ void ckpy_d(double * rho, double * T, double * y_wk, double * P)
{
    double imw[29];/* inv molecular weight array */
    imolecularWeight_d(imw);
    double YOW = 0;/* for computing mean MW */
    YOW += y_wk[0]*imw[0]; /*NC7H16 */
    YOW += y_wk[1]*imw[1]; /*O2 */
    YOW += y_wk[2]*imw[2]; /*CO2 */
    YOW += y_wk[3]*imw[3]; /*H2O */
    YOW += y_wk[4]*imw[4]; /*CO */
    YOW += y_wk[5]*imw[5]; /*H2 */
    YOW += y_wk[6]*imw[6]; /*OH */
    YOW += y_wk[7]*imw[7]; /*H2O2 */
    YOW += y_wk[8]*imw[8]; /*HO2 */
    YOW += y_wk[9]*imw[9]; /*H */
    YOW += y_wk[10]*imw[10]; /*O */
    YOW += y_wk[11]*imw[11]; /*CH3O */
    YOW += y_wk[12]*imw[12]; /*CH2O */
    YOW += y_wk[13]*imw[13]; /*HCO */
    YOW += y_wk[14]*imw[14]; /*CH2 */
    YOW += y_wk[15]*imw[15]; /*CH3 */
    YOW += y_wk[16]*imw[16]; /*CH4 */
    YOW += y_wk[17]*imw[17]; /*C2H3 */
    YOW += y_wk[18]*imw[18]; /*C2H4 */
    YOW += y_wk[19]*imw[19]; /*C2H5 */
    YOW += y_wk[20]*imw[20]; /*C3H4 */
    YOW += y_wk[21]*imw[21]; /*C3H5 */
    YOW += y_wk[22]*imw[22]; /*C3H6 */
    YOW += y_wk[23]*imw[23]; /*C3H7 */
    YOW += y_wk[24]*imw[24]; /*C7H15-2 */
    YOW += y_wk[25]*imw[25]; /*C7H15O2 */
    YOW += y_wk[26]*imw[26]; /*C7KET12 */
    YOW += y_wk[27]*imw[27]; /*C5H11CO */
    YOW += y_wk[28]*imw[28]; /*N2 */
    *P = *rho * 8.31451e+07 * (*T) * YOW; /*P = rho*R*T/W */

    return;
}


/*Compute rho = P*W(y)/RT */
__device__ void ckrhoy_d(double * P, double * T, double * y_wk, double * rho)
{
    double YOW = 0;
    double imw[29];/* inv molecular weight array */
    imolecularWeight_d(imw);
    double tmp[29];

    for (int i = 0; i < 29; i++)
    {
        tmp[i] = y_wk[i]*imw[i];
    }
    for (int i = 0; i < 29; i++)
    {
        YOW += tmp[i];
    }

    *rho = *P / (8.31451e+07 * (*T) * YOW);/*rho = P*W/(R*T) */
    return;
}


/*convert y[species] (mass fracs) to c[species] (molar conc) */
__device__ void ckytcr_d(double * rho, double * T, double * y_wk, double * c)
{
    double imw[29];/* inv molecular weight array */
    imolecularWeight_d(imw);
    for (int i = 0; i < 29; i++)
    {
        c[i] = (*rho)  * y_wk[i] * imw[i];
    }
}


